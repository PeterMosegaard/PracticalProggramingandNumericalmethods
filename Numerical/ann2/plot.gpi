set terminal svg background "white"
set output "plot.svg"
set xlabel "y"
set ylabel "x"
set key noenhanced
set key top
set grid
set tics out
set title "ANN for interpolation"

plot 'out.txt' using 1:2 title "Data points", 'ANN.txt' using 1:2 with lines title "ANN interpolation", 'ANN.txt' using 1:3 with lines title "ANN derivative", 'ANN.txt' using 1:4 with lines title "ANN anti derivative"
